<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="item">
	<!-- 컬럼명과 property명을 일치-->
	<resultMap type="ItemVo" id="itemMap">
		<id column="item_num" property="itemNo"/>
		<result column="item_img" property="itemImg"/>
		<result column="item_price" property="itemPrice"/>
		<result column="item_store" property="itemStore"/>
		<result column="item_recom" property="itemRecom"/>
		<result column="item_date" property="itemDate"/>
		<result column="cate_num" property="cateNo"/>
		<result column="board_num" property="boardNo"/>
		<result column="mem_num" property="memNo"/>
	</resultMap> 

</mapper>









