 @font-face{
 	font-family:'a옛날목욕탕L';
 	src:local('a옛날목욕탕L') format('opentype'),
 }
  @font-face{
 	font-family:'a어린이날L';
 	src:local('a어린이날L') format('opentype'),
 }
  @font-face{
 	font-family:'나눔바른고딕';
 	src:local('나눔바른고딕') format('truetype'),
 }

body {
    padding-top: 70px; /* Required padding for .navbar-fixed-top. Remove if using .navbar-static-top. Change if height of navigation changes. */
}

.portfolio-item {
    margin-bottom: 25px;
}

footer {
    margin: 0;
}

.footer-bottom-wrapper{
	background:#3B3F46;
	line-height:40px;
	border-top:1px solid #888;
	
}

#myCarousel{
	width:944px;
}

.carousel-control{
	font-family:"Helvetica Neue", Helvetica, Arial, sans-serif;
}

.btn-image{
	  color: #37373B;
	  background-color:#fff;
	  margin-top:1px;
	  margin-botton:auto;
	  border-radius: 0px;
	  border:1px solid #D1D1D1;
	  font-size:18px;
}

.btn-nav{
	  color: #fff;
	  background-color:#D9534F;
	  margin-top:auto;
	  margin-botton:auto;
	  border-radius: 4px;
	  border:1px solid #D9534F;
	  font-size:18px;
	  font-family:나눔바른고딕;
}

.btn-nav:hover,
.btn-nav:focus,
.btn-nav:active,
.btn-nav.active,
.open > .dropdown-toggle.btn-nav {
  color: #fff;
  background-color: #D9534F;
  border-color: #D9534F;
}
